{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
    "globals": {
    "document": false,
    "navigator": false,
    "window": false,
    "jQuery" : true,
    "$" : true
  },

  "rules": {
    "quotes": "off",
    "no-trailing-spaces": "off",
    "no-global-assign": "error",
    "no-unused-vars": "off",
    "no-underscore-dangle": "off",
    "no-alert": "off",
    "no-lone-blocks": "off",
    "camelcase" : "warn",
    "strict" : ["warn", "global"]
  }
}